{
    "name": "UX Query Tool",
    "description": "Extension for fast query using User Explorer",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs", "activeTab", "scripting", "storage"],
    "action": {
        "default_popup": "popup.html"
    },
      "icons": {
        "16": "/images/UX_logo.png",
        "32": "/images/UX_logo.png",
        "48": "/images/UX_logo.png",
        "128": "/images/UX_logo.png"
      }
    
  }