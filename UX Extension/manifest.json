{
    "name": "UX Query Extension",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs", "activeTab", "scripting", "storage","webNavigation"],
    "action": {
        "default_popup": "popup.html"
    },
      "icons": {
        "16": "/images/dbicon.png",
        "32": "/images/dbicon.png",
        "48": "/images/dbicon.png",
        "128": "/images/dbicon.png"
      }
    
  }